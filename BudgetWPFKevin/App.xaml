<Application x:Class="BudgetWPFKevin.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:BudgetWPFKevin"
             xmlns:vm="clr-namespace:BudgetWPFKevin.ViewModels"
             xmlns:views="clr-namespace:BudgetWPFKevin.Views"
             xmlns:converters="clr-namespace:BudgetWPFKevin.Converters">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Styles.xaml"/>
                <ResourceDictionary Source="/Resources/DataTemplates.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
